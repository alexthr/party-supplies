<div class="wrapper-booking">
  <div class="container">
    <h1 class="color-font">Your bookings</h1>
    <div class="row">
      <% @bookings.each do |booking| %>
        <div class="col-xs-6 col-sm-4">
          <%= render 'card', booking: booking %>
        </div>
      <% end %>
    </div>
  </div>
</div>
